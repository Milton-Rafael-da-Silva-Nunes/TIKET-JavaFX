<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass"
    xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="view.controller.FXMLTelaPrincipalController">
    
    <stylesheets>
        /view/css/fxmltelaprincipal.css
    </stylesheets>

    <!-- Usando um HBox para centralizar os botões -->
    <HBox spacing="20.0" alignment="CENTER" prefHeight="600.0" prefWidth="800.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <children>
            <Button fx:id="buttonSocio" mnemonicParsing="false" onAction="#testeButton" prefHeight="58.0" prefWidth="204.0" text="Sócios" />
            <Button fx:id="buttonTiket" mnemonicParsing="false" onAction="#testeButton" prefHeight="58.0" prefWidth="204.0" text="Tickets" />
            <Button fx:id="buttonValidacao" mnemonicParsing="false" onAction="#testeButton" prefHeight="58.0" prefWidth="204.0" text="Validação" />
        </children>
    </HBox>
</AnchorPane>
